{
    "require": {
        "doctrine/dbal": "^2.12",
        "doctrine/migrations": "^3.0",
        "intervention/image": "^2.5",
        "league/flysystem": "^2.0",
        "league/flysystem-google-cloud-storage": "^2.0",
        "silex/silex": "^2.3",
        "symfony/browser-kit": "^4.4",
        "symfony/console": "^5.2",
        "symfony/error-handler": "^5.2",
        "symfony/twig-bridge": "^3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
